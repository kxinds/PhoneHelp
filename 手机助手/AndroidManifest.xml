<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.phone"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="18" />

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.KILL_BACKGROUND_PROCESSES" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.phone.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.phone.HomeActivity"
            android:label="@string/title_activity_home" >
        </activity>
        <activity
            android:name="com.example.phone.LocalHomeActivity"
            android:label="@string/title_activity_local_home" >
        </activity>
        <activity
            android:name="com.example.phone.PhoneSpeedActivity"
            android:label="@string/title_activity_phone_speed" >
        </activity>
        <activity
            android:name="com.example.phone.PhoneManageActivity"
            android:label="@string/title_activity_phone_manage" >
        </activity>
        <activity
            android:name="com.example.phone.PhoneCheckActivity"
            android:label="@string/title_activity_phone_check" >
        </activity>
        <activity
            android:name="com.example.phone.TelBookActivity"
            android:label="@string/title_activity_tel_book" >
        </activity>
        <activity
            android:name="com.example.phone.PhoneCleanActivity"
            android:label="@string/title_activity_phone_clean" >
        </activity>
        <activity
            android:name="com.example.phone.AllProgrameActivity"
            android:label="@string/title_activity_all_programe" >
        </activity>
        <activity
            android:name="com.example.phone.Tel_ManageActivity"
            android:label="@string/title_activity_tel__manage" >
        </activity>
        <activity
            android:name="com.example.phone.SysProgrameActivity"
            android:label="@string/title_activity_sys_programe" >
        </activity>
        <activity
            android:name="com.example.phone.UserProgramActivity"
            android:label="@string/title_activity_user_program" >
        </activity>
        <activity
            android:name="com.example.phone.ProgramMainActivity"
            android:label="@string/title_activity_program_main" >
        </activity>
    </application>

</manifest>
